.temp-white{
  background-color: white;
  height: 100vh;
}

.orderComplete-top{
  background-color: white;
}

.orderComplete-toolbar{
  position: relative;
}


.title-bar{
  height: 48px;
  display: flex;
  flex-direction: row;
}

.cancle-button-box{
  width: 48px;
  height: 48px;
  align-content: left;
}

.cancle-button{
  width: 48px;
  height: 48px;
}

.title-bar-text{
  font-size: 16px;
  font-weight: 500;
  color: #333333;
  margin-top: 15px;
}

.border{
  height: 1px;
  background-color: #dddddd;
}

.order-complete-text-box{
  height: 46px;
  display: flex;
  margin-top: 54px;
}

.order-complete-text{
  font-size: 34px;
  font-weight: 600;
  color: #333333;
  width: 150px;
  margin: 0 auto;
}

.order-accept-box{
  height: 24px;
  margin-top: 24px;
}

.order-accept-text{
  font-size: 18px;
  font-weight: normal;
  color: #333333;
  width: 220px;
  margin: 0 auto;
}

.receipt-area{
  height: 240px;
  margin-top: 50px;
  position: relative;
}

.receipt-box{
  width: 200px;
  height: 240px;
  margin-left: 17%;
  z-index: 1;
  position: absolute;
}

.cafe-name-text{
  font-size: 18px;
  font-weight: normal;
  color: #333333;
  z-index: 2;
  position: absolute;
  margin: 54px 0px 0px 127px;
}

.no-text{
  font-size: 30px;
  color: #333333;
  z-index: 2;
  position: absolute;
  margin: 126px 0px 0px 127px;
}

.number-text{
  font-size: 60px;
  font-weight: bold;
  color: #333333;
  z-index: 2;
  position: absolute;
  margin: 100px 0px 0px 179px;
}

footer{ 
  position:fixed; 
  left:0px; 
  bottom:0px; 
  height:42px;
  width:100%; 
  background-color: white; 
  }

.go-home-text{
  font-size: 19px;
  font-weight: normal;
  color: #0b463c;
  width: 128px;
  margin: 0 auto;
}